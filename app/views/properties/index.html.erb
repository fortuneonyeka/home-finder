<p style="color: green"><%= notice %></p>
<div class="property-index">
<h1 class="mt-4">Properties</h1>

<div id="properties">
  <% @properties.each do |property| %>
    <%= render property %>
    <p>
      <%= link_to "Show this property", property %>
    </p>
  <% end %>
</div>
</div>

<%= link_to "New property", new_property_path %>
